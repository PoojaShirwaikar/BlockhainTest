package com.psl.assignment1;

public class Ticket {

	private Train train;
	private String time;
	private String price;
	private String source;
	private String destination;

	public String getTrain() {
		return train;
	}

	public void setTrain(String train) {
		this.train = train;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getSource() {
		return source;
	}

	public void setSource(String source) {
		this.source = source;
	}

	public String getDestination() {
		return destination;
	}

	public void setDestination(String destination) {
		this.destination = destination;
	}

	@Override
	public String toString() {
		return "Ticket [train=" + train + ", time=" + time + ", price=" + price
				+ ", source=" + source + ", destination=" + destination + "]";
	}

	public Ticket(String train, String time, String price, String source,
			String destination) {
		System.out.println("Parametrized constructor:ticket");
		this.train = train;
		this.time = time;
		this.price = price;
		this.source = source;
		this.destination = destination;
	}

	public Ticket() {
		System.out.println("default constructor:ticket");

	}
	

}
