package com.tienda.bean;

public class Product {
	private int produvtId;
	private String code, name, description;
	private int price;

	public Product() {

	}

	public Product(int produvtId, String code, String name, String description,
			int price) {
		super();
		this.produvtId = produvtId;
		this.code = code;
		this.name = name;
		this.description = description;
		this.price = price;
	}

}
